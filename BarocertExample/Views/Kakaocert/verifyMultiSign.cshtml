@{
	ViewBag.Title = "전자서명 서명검증 (다건)";
}

<div id="content">
	<p class="heading1">Result</p>
	<br />
	<fieldset class="fieldset1">
		<legend>@ViewData["Title"]</legend>
        <ul>
            <li>ReceiptID (접수 아이디) : @Model.receiptID</li>
            <li>RequestID (요청 아이디) : @Model.requestID</li>
            <li>State (상태) : @Model.state</li>

            @{
                foreach (var bulkResult in Model.bulkSignedData) {

                    <li>BulkSignedData (전자서명 데이터 전문) : @Model.bulkResult</li>

                }
            }

            <li>Ci (연계정보) : @Model.ci</li>
        </ul>
	</fieldset>
</div>
